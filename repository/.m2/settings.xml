<settings xmlns="http://maven.apache.org/SETTINGS/1.0.0"
            xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
            xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.0.0 http://maven.apache.org/xsd/settings-1.0.0.xsd">
    <servers>
        <server>
            <id>cosmocode-private-releases</id>
            <username>wsho</username>
            <password>schoenborn</password>
        </server>
        <server>
            <id>palava-website</id>
            <username>mvn</username>
            <privateKey>/home/wsho/.ssh/mvn</privateKey>
        </server>
        <server>
            <id>cosmocode-public-releases</id>
            <username>mvn</username>
            <privateKey>/home/wsho/.ssh/mvn</privateKey>
        </server>
        <server>
            <id>cosmocode-public-snapshots</id>
            <username>mvn</username>
            <privateKey>/home/wsho/.ssh/mvn</privateKey>
        </server>
    </servers>
    <mirrors>
        <mirror>
            <id>mvn.cosmo</id>
            <mirrorOf>*</mirrorOf>
            <url>http://mvn.cosmo/content/groups/public/</url>
        </mirror>
    </mirrors>
    <activeProfiles>
        <activeProfile>mvn.cosmo</activeProfile>
    </activeProfiles>
    <profiles>
        <profile>
            <id>mvn.cosmo</id>
            <repositories>
                <repository>
                    <id>central</id>
                    <url>http://mvn.cosmo/content/groups/public/</url>
                    <snapshots>
                        <enabled>true</enabled>
                    </snapshots>
                </repository>
            </repositories>
            <pluginRepositories>
                <pluginRepository>
                    <id>central</id>
                    <url>http://mvn.cosmo/content/groups/public/</url>
                    <snapshots>
                        <enabled>true</enabled>
                    </snapshots>
                </pluginRepository>
            </pluginRepositories>
        </profile>
        <profile>
        </profile>
        <profile>
            <id>mvn.cosmocode.de</id>
            <repositories>
                <repository>
                    <id>mvn.cosmocode.de</id>
                    <url>http://mvn.cosmocode.de</url>
                </repository>
            </repositories>
            <pluginRepositories>
                <pluginRepository>
                    <id>mvn.cosmocode.de</id>
                    <url>http://mvn.cosmocode.de/</url>
                </pluginRepository>
            </pluginRepositories>
        </profile>
    </profiles>
</settings>
